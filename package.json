{
  "name": "webpack-node-boilerplate",
  "version": "0.1.0",
  "description": "Framework agnostic boilerplate for webpack, babel, node.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "./node_modules/.bin/webpack --config ./webpack.prod.config.js --progress --colors",
    "clean": "rm -rf build/",
    "start": "./node_modules/.bin/nodemon server.js",
    "postinstall": "./node_modules/.bin/webpack -p --config webpack.prod.config.js"
  },
  "author": "Griffin Michl <griffinmichl@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "express": "^4.13.4",
    "nodemon": "^1.9.2",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
